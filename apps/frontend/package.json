{
  "name": "astrobalendar-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint --config eslint.config.cjs --ext .ts,.tsx src",
    "lint:fix": "eslint --config eslint.config.cjs --ext .ts,.tsx --fix src",
    "sass:dev": "sass --watch src/styles:src/styles --style=expanded",
    "sass:build": "sass src/styles:dist/styles --no-source-map --style=compressed",
    "preview:prod": "vite preview --host",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint",
    "prepare": "[ -z \"$CI\" ] && husky install || echo 'Skipping husky install in CI'",
    "predeploy": "npm run build",
    "deploy": "netlify deploy --prod"
  },
